function loadBubbles() {
 //add bubbles to container
 var makeBubbles = $("<img class='box'>")
 makeBubbles.prop("src", "img/bubble-04.jpg");
 $(".grid_container").append(makeBubbles);

 //set the bubble to drop on x-axis for the getRandomInt function
 var xPos = getRandomInt(0, 700);
 makeBubbles.css("left", xPos + "px");
 makeBubbles.css("top", "-30px");

 //set bubbles to drop speed for getRandomInt function
 var speed = getRandomInt(5000, 10000);

 //bubbles fall
 makeBubbles.animate({"top": "630px"}, speed, "swing", makeSucculent)




 //listener for event clicks and remove image
 makeBubbles.click(bubblesClick);
 function bubblesClick() {
 $(this).remove();
